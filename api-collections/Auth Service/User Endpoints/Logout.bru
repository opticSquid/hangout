meta {
  name: Logout
  type: http
  seq: 3
}

delete {
  url: {{auth_base_url}}/user/logout
  body: none
  auth: bearer
}

headers {
  os: ubuntu/linux
  Screen-Width: 1920
  Screen-Height: 1080
}

auth:bearer {
  token: {{access_token}}
}

script:post-response {
  bru.deleteEnvVar("access_token");
  bru.deleteEnvVar("refresh_token");
}
