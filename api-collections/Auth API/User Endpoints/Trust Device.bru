meta {
  name: Trust Device
  type: http
  seq: 2
}

post {
  url: {{auth_base_url_dev}}/user/trust-device
  body: none
  auth: bearer
}

headers {
  OS: ubuntu/linux
  Screen-Height: 1920
  Screen-Width: 1080
}

auth:bearer {
  token: {{access_token}}
}

script:post-response {
  bru.setEnvVar("access_token",res.body.accessToken);
  bru.setEnvVar("refresh_token",res.body.refreshToken);
}
