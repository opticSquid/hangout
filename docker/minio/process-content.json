{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "StorageServiceUploadPathAccess",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::upload",
                "arn:aws:s3:::upload/*"
            ]
        },
        {
            "Sid": "StorageServiceStoragePathAccess",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:s3:::processed",
                "arn:aws:s3:::processed/*"
            ]
        }
    ]
}