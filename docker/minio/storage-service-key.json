{
 "Version": "2012-10-17",
 "Statement": [
  {
   "Sid": "StorageServiceUploadPathAccess",
   "Effect": "Allow",
   "Action": [
    "s3:GetObject",
    "s3:ListBucket"
   ],
   "Resource": [
    "arn:aws:s3:::hangout-upload-path",
    "arn:aws:s3:::hangout-upload-path/*"
   ]
  },
  {
   "Sid": "StorageServiceStoragePathAccess",
   "Effect": "Allow",
   "Action": [
    "s3:ListBucket",
    "s3:PutObject"
   ],
   "Resource": [
    "arn:aws:s3:::hangout-storage-path",
    "arn:aws:s3:::hangout-storage-path/*"
   ]
  }
 ]
}
